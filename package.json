{
  "name": "discord-music-bot",
  "version": "1.0.0",
  "description": "A Discord bot that plays music from Spotify and YouTube.",
  "main": "src/bot.ts",
  "scripts": {
    "prestart": "npm run build",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/bot.ts",
    "prod": "npm run build && npm run start",
    "start": "node dist/bot.js"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "@distube/ytdl-core": "^4.16.12",
    "discord.js": "^14.24.2",
    "dotenv": "^10.0.0",
    "ffmpeg-static": "^4.4.1",
    "opusscript": "^0.0.8",
    "play-dl": "^1.9.7",
    "puppeteer": "^24.31.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "spotify-web-api-node": "^5.0.0",
    "youtube-dl-exec": "^3.0.26",
    "yt-dlp-exec": "^1.0.2",
    "yt-search": "^2.13.1",
    "ytpl": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "spotify",
    "youtube"
  ],
  "author": "Your Name",
  "license": "MIT"
}
